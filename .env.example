# Highrise Bot Configuration
# Copy this file to .env and fill in your actual values

# Your Highrise bot token (get this from Highrise)
BOT_TOKEN=your_bot_token_here

# The room ID where your bot will operate
ROOM_ID=your_room_id_here

# Your bot's username (optional, defaults to 'MyBot')
BOT_USERNAME=MyBot

# Enable debug mode (optional, defaults to False)
DEBUG=False
